        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.sensitive.mapper.SensitiveWordMapper">

    <resultMap id="BaseResultMap" type="com.example.monitor.domain.po.SensitiveWord">
